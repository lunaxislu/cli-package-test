#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/tsup@8.2.4_typescript@5.5.4/node_modules/tsup/dist/node_modules:/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/tsup@8.2.4_typescript@5.5.4/node_modules/tsup/node_modules:/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/tsup@8.2.4_typescript@5.5.4/node_modules:/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/tsup@8.2.4_typescript@5.5.4/node_modules/tsup/dist/node_modules:/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/tsup@8.2.4_typescript@5.5.4/node_modules/tsup/node_modules:/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/tsup@8.2.4_typescript@5.5.4/node_modules:/mnt/c/Users/최문길/Desktop/cli-test/cli-button/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../dist/cli-default.js" "$@"
fi
